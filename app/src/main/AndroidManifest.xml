<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wzc.coordinatorlayout.examples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BottomSheetBehaviorActivity"
            android:label="@string/bottomsheet_behavior_activity_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".FABActivity"
            android:label="@string/fab_activity_name">
        </activity>
        <activity
            android:name=".CollapsingToolbarLayoutActivity"
            android:label="@string/collapsingtoolbarlayout_activity_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".CollapsingToolbarLayoutActivity2"
            android:label="@string/collapsingtoolbarlayout_activity_2_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".CollapsingToolbarLayoutActivity3"
            android:label="@string/collapsingtoolbarlayout_activity_3_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".CollapsingToolbarLayoutActivity4"
            android:label="@string/collapsingtoolbarlayout_activity_4_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".AppBarLayoutActivity"
            android:label="@string/appbarlayout_activity_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ScrollBehaviorActivity"
            android:label="@string/scroll_behavior_activity_name">
        </activity>
        <activity
            android:name=".DependentBehaviorActivity"
            android:label="@string/dependent_behavior_activity_name">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>